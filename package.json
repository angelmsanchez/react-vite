{
  "name": "react-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build:prod": "tsc && vite build --mode prod",
    "build:watch": "tsc && vite build --watch",
    "build": "tsc && vite build --mode development",
    "dev": "vite",
    "preview": "vite preview",
    "serve:external": "http-server dist -p 3000 --cors -o",
    "serve:prod": "npm run build:prod && vite preview",
    "serve": "npm run build && vite preview",
    "start": " npm run dev --host"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.5.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "sass": "^1.57.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  }
}
