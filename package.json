{
  "name": "react-vite",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "start": " npm run dev --host",
    "build": "tsc && vite build --mode development",
    "build:watch": "tsc && vite build --watch",
    "build:prod": "tsc && vite build --mode prod",
    "serve": "npm run build && vite preview",
    "serve:external": "http-server dist -p 3000 --cors -o",
    "serve:prod": "npm run build:prod && vite preview"
  },
  "dependencies": {
    "dotenv": "^16.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@vitejs/plugin-react": "^1.0.7",
    "http-server": "^14.1.0",
    "rollup-plugin-visualizer": "^5.6.0",
    "sass": "^1.49.10",
    "typescript": "^4.5.4",
    "vite": "^2.9.0"
  }
}
